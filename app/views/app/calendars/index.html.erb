    <%= icon('fa-solid','plus',class:'btn-icon') %>
    <%= App::Calendar.model_name.human %>
  <% end %>
<% end %>

<% provide :head_title, App::Calendar.model_name.human.pluralize %>
<% provide :page_title, App::Calendar.model_name.human.pluralize %>
