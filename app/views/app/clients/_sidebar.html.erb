
<aside id="sidebar-multi-level-sidebar-b" class=" fixed top-0 right-0 z-40 h-screen overflow-y-auto transition-transform translate-x-full xl:translate-x-0 bg-white w-64 dark:bg-gray-800"  aria-label="Sidebar">
  <div class="h-full px-3 overflow-y-auto  scrollbar-hide">

    

    <div class="space-y-1 my-4">       
      
      
      <div class="detail-input flex justify-between items-center">
        <p class=""><%= icon('fa-solid','building-user')%> <%= App::Client.model_name.human.pluralize %> </p>
        <p class="px-2 py-1 text-xs font-medium text-white bg-blue-600 rounded-full"> <%= App::Client.only_active.count %> / <%= App::Client.all.count %> </p>
      </div>

      <div class="detail-input flex justify-between items-center">
        <p class=""><%= icon('fa-solid','key')%> <%= App::Client.human_attribute_name('licenses')%> </p>
        <p class="px-2 py-1 text-xs font-medium text-white bg-blue-600 rounded-full"> <%= count_userd_licenses %> /  <%= count_active_licenses %> </p>
