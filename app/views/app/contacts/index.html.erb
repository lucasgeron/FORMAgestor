<% content_for :ctas_primary do %>
  <%= link_to new_app_contact_path, class: "btn-primary flex-none" do %>
    <%= icon('fa-solid','plus',class:'btn-icon') %>
    <%= App::Contact.model_name.human %>
  <% end %>
<% end %>

<% provide :head_title, App::Contact.model_name.human(count:2) %>
<% provide :page_title, App::Contact.model_name.human(count:2) %>
<% provide :sidebar, render(partial: 'app/contacts/sidebar') %>

<% content_for :page_content do %>
  <%= render partial: "contacts" %>
<%end%>
