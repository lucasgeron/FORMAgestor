    <%= icon('fa-solid','arrow-left', class:'-mx-1')%>
  <%end%>

  <%= link_to app_role_vendor_path(@app_role_vendor), class: "btn-secondary flex-none" do %>
    <%= icon('fa-solid','arrow-up-right-from-square', class:'-mx-1')%>
  <%end%>
<% end %>

<% provide :head_title, "#{t('views.app.general.editing')} #{App::StatusInteraction.model_name.human}" %>
