<% content_for :ctas_secondary do %>
  <%= link_to app_status_negotiations_path, class: "btn-secondary-gray flex-none" do %>
    <%= icon('fa-solid','arrow-left', class:'-mx-1')%>
  <%end%>

  <%= link_to app_status_negotiation_path(@app_status_negotiation), class: "btn-secondary flex-none" do %>
    <%= icon('fa-solid','arrow-up-right-from-square', class:'-mx-1')%>
  <%end%>
<% end %>

<% provide :head_title, "#{t('views.app.general.editing')} #{App::StatusNegotiation.model_name.human}" %>
<% provide :page_title, "#{t('views.app.general.editing')} #{App::StatusNegotiation.model_name.human}" %>
<% provide :entry_id, "#{@app_status_negotiation.id}" %>

<% content_for :page_content do %>
  <%= render "form", app_status_negotiation: @app_status_negotiation %>
<%end%>
