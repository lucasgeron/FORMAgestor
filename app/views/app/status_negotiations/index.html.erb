<% content_for :ctas_primary do %>
 
  <%= link_to new_app_status_negotiation_path, class: "btn-primary flex-none" do %>
    <%= icon('fa-solid','plus',class:'btn-icon') %>
    <%= App::StatusNegotiation.model_name.human %>
  <% end %>
<% end %>

<% content_for :ctas_secondary do %>
  <%= link_to app_settings_path, class: "btn-secondary-gray flex-none" do %>
    <%= icon('fa-solid','arrow-left', class:'-mx-1')%>
  <%end%>
<%end%>

<% provide :head_title, App::StatusNegotiation.model_name.human(count:2) %>
<% provide :page_title, App::StatusNegotiation.model_name.human(count:2) %>
<% provide :sidebar, render(partial: 'app/status_negotiations/sidebar') %>

<% content_for :page_content do %>

