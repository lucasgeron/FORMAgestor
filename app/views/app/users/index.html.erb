<% content_for :ctas_primary do %>
  <%= link_to new_app_user_path, class: "btn-primary flex-none" do %>
    <%= icon('fa-solid','plus',class:'btn-icon') %>
    <%= User.model_name.human %>
  <% end %>
<% end %>

   
<% provide :head_title, User.model_name.human.pluralize %>
<% provide :page_title, User.model_name.human.pluralize %>
<% provide :sidebar, render(partial: 'app/users/sidebar') %>

<% content_for :page_content do %>
  <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-3 2xl:grid-cols-4 gap-4">
    <%= render(@users)%>
  </div>
<%end%>
